<html>
    <head>
        <title><%= title %></title>
        <link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
  <link href="https://fonts.googleapis.com/css2?family=Open+Sans&display=swap" rel="stylesheet">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" >
  <link href="/assets/css/style2.css" rel="stylesheet" type="text/css" >
     
    </head>
    <body>

      <!-- addition  of navbar -->
        <%- include('./partials/navbar') %>
      <!-- End of navbar -->
      <!-- begin side bar  -->
      <br><br>
        <div class="section">
            <div class="row">
                <div class="col-sm-2"  >
                
                   <%- include('./partials/sidebar') %>
                
            </div>
                <div class="col-sm-10"  >
                    <div class="content">
                        <div class="container"><br><br>
                            <h3>Dashboard</h3>
                            <div class="row mt-5 container">
                                
                                <div class="col-xl-4 col-md-3 mb-3  " data-toggle="modal" data-target="#exampleModalLong" >
                                  <div class="card shadow-sm h-100 py-2">
                                    <div class="card-body">
                                      <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                          <div class="text-xs font-weight-bold text-success text-uppercase mb-1">Upload a Blog Post</div>
                                          <div class="h5 mb-0 font-weight-bold text-gray-800" >5000</div>
                                        </div>
                                        <div class="col-auto">
                                          <i class="fas fa-user fa-3x text-gray-300"></i>
                                        </div>
                                      </div>
                                    </div>
                                  </div>
                        
                                </div>
                                
                               
                                  
                        
                                <div class="col-xl-4 col-md-3 mb-3 " data-toggle="modal" data-target="#exampleModalLong2">
                                  <div class="card  shadow-sm h-100 py-2">
                                    <div class="card-body">
                                      <div class="row no-gutters align-items-center">
                                        <div class="col mr-2">
                                          <div class="text-xs font-weight-bold text-secondary text-uppercase mb-1">Upload a testimonial</div>
                                          <div class="h5 mb-0 font-weight-bold text-gray-800">50% </div>
                                        </div>
                                        <div class="col-auto">
                                          <i class="fas fa-tasks fa-3x text-gray-300"></i>
                                        </div>
                                      </div>
                                    </div>
                                  </div>
                        
                                </div>
                        
                       
                        
                                <div class="col-xl-4 col-md-3 mb-3  " data-toggle="modal" data-target="#exampleModalLong3">
                                    <div class="card shadow-sm h-100 py-2">
                                      <div class="card-body">
                                        <div class="row no-gutters align-items-center">
                                          <div class="col mr-2">
                                            <div class="text-xs font-weight-bold text-success text-uppercase mb-1">upload a Portfolio</div>
                                            <div class="h5 mb-0 font-weight-bold text-gray-800">5000</div>
                                          </div>
                                          <div class="col-auto">
                                            <i class="fas fa-user fa-3x text-gray-300"></i>
                                          </div>
                                        </div>
                                      </div>
                                    </div>
                          
                                  </div>
                        
                              </div>

                              <div class="row container">
        
                                <div class="col-xl-8">
                                  <div class="card mb-3">
                                    <div class="card-header text-center text-uppercase font-weight-bold"   >Projects</div>
                                    <div class="card-body">
                                      <h4 class="small font-weight-bold">Server Migration <span class="float-right">20%</span></h4>
                                      <div class="progress mb-4">
                                        <div class="progress-bar bg-danger" role="progressbar" style="width: 20%" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                                      </div>
                                      <h4 class="small font-weight-bold">Customer Database <span class="float-right">50%</span></h4>
                                      <div class="progress mb-4">
                                        <div class="progress-bar bg-warning" role="progressbar" style="width: 50%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
                                      </div>
                                      <h4 class="small font-weight-bold">Page Maintenance <span class="float-right">60%</span></h4>
                                      <div class="progress mb-4">
                                        <div class="progress-bar bg-dark" role="progressbar" style="width: 60%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
                                      </div>
                                      <h4 class="small font-weight-bold">Forms Submission <span class="float-right">80%</span></h4>
                                      <div class="progress mb-4">
                                        <div class="progress-bar bg-info" role="progressbar" style="width: 80%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
                                      </div>
                                      <h4 class="small font-weight-bold">Project Upload <span class="float-right">100%</span></h4>
                                      <div class="progress mb-4">
                                        <div class="progress-bar bg-success" role="progressbar" style="width: 100%" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100"></div>
                                      </div>
                                    </div>
                                  </div>
                        
                                </div>
                                <div class="col-sm-4 ">
                                    <div class="card text-white bg-danger mb-3 pb-4" style="width:100%;">
                                      <div class="card-body">
                                        <h2 class="card-title mt-2" style="font-weight: bolder;">123</h2>
                                        <p class="card-text">Comments </p>
                                          
                                      </div>
                                    </div>
                                    <div class="card text-white bg-dark mb-3 pb-4" style="width:100%;">
                                      <div class="card-body">
                                        <h2 class="card-title mt-2" style="font-weight: bolder;">214</h2>
                                        <p class="card-text">New Followers</p>
                                      </div>
                                    </div>
                                  </div>
                        
                              
                              
                            
                            
                            
                              </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

       
      <!-- end of sidebar -->
